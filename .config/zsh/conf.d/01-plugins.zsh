# Make sure your terminal is in 256-color mode
export TERM="xterm-256color"
# 256-color prompt support
source $ZDOTDIR/plugins/zsh-256color/zsh-256color.plugin.zsh
# Autosuggestions (must come before syntax-highlighting)
source $ZDOTDIR/plugins/zsh-autosuggestions/zsh-autosuggestions.zsh
# Syntax highlighting (last)
source $ZDOTDIR/plugins/zsh-syntax-highlighting/zsh-syntax-highlighting.zsh
